@model IEnumerable<FeedbackViewModel>

@{
    ViewData["Title"] = "Feedback";
}


<div class="text-center">
  <h1>Feedback</h1>
  <p>Here is some of the feedback we have recieved from entries!</p>
</div>

<table class="table">
    <thead>
        <tr><th>Name</th><th>Response</th></tr>
    </thead>
    <tbody>
        @foreach (FeedbackViewModel r in Model)
        {
            <tr>
                <td>@r.FeedbackName</td>
                <td>@r.FeedbackText</td>
            </tr>
        }
    </tbody>
</table>